{
  "name": "loadjs-torrent",
  "version": "3.0.2",
  "author": "Kayleepop",
  "license": "MIT",
  "description": "An extension of loadjs that supports torrents",
  "scripts": {
    "test": "standard && browserify test/test.js | tape-run --static test/fixtures | tap-spec",
    "build": "browserify loadjs-torrent.js -s torrentJs | minify > loadjs-torrent.min.js"
  },
  "main": "loadjs-torrent.js",
  "keywords": [
    "loadjs",
    "torrent",
    "webtorrent",
    "peer",
    "p2p",
    "load",
    "balance"
  ],
  "repository": "https://github.com/kayleepop/loadjs-torrent.git",
  "bugs": {
    "type": "git",
    "url": "https://github.com/kayleepop/loadjs-torrent/issues"
  },
  "devDependencies": {
    "babel-minify": "^0.5.1",
    "browserify": "^16.1.1",
    "muggle-assert": "^1.0.0",
    "muggle-test": "^1.0.0",
    "standard": "^13.0.0",
    "tap-spec": "^5.0.0",
    "tape-run": "^4.0.0"
  },
  "dependencies": {
    "glob-to-regexp": "^0.4.0",
    "idbkv-chunk-store": "^1.0.2",
    "loadjs": "^3.5.3",
    "webtorrent": "^0.107.0"
  },
  "standard": {
    "ignore": [
      "test/fixtures/"
    ]
  }
}
