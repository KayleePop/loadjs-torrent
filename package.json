{
  "name": "loadjs-torrent",
  "version": "2.1.1",
  "author": "Kayleepop",
  "license": "MIT",
  "description": "An extension of loadjs that supports torrents",
  "scripts": {
    "test": "standard && browserify test/*.js | tape-run",
    "build": "browserify -t [ babelify --presets [ env ] ] loadjs-torrent.js -s torrentJs | minify > loadjs-torrent.min.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  },
  "main": "loadjs-torrent.js",
  "keywords": [
    "loadjs",
    "torrent",
    "webtorrent",
    "peer",
    "p2p",
    "load",
    "balance"
  ],
  "repository": "https://github.com/kayleepop/loadjs-torrent.git",
  "bugs": {
    "type": "git",
    "url": "https://github.com/kayleepop/loadjs-torrent/issues"
  },
  "devDependencies": {
    "babel-minify": "^0.3.0",
    "browserify": "^16.1.1",
    "standard": "^11.0.1",
    "tape": "^4.6.3",
    "tape-run": "^3.0.1"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babelify": "^8.0.0",
    "babel-preset-env": "^1.6.1",
    "glob-to-regexp": "^0.4.0",
    "loadjs": "^3.5.3",
    "webtorrent": "^0.98.18"
  },
  "standard": {
    "ignore": [
      "test/fixtures/"
    ]
  }
}
