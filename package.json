{
  "name": "loadjs-torrent",
  "version": "2.2.1",
  "author": "Kayleepop",
  "license": "MIT",
  "description": "An extension of loadjs that supports torrents",
  "scripts": {
    "test": "standard && browserify test/*.js | tape-run",
    "build": "browserify loadjs-torrent.js -s torrentJs | minify > loadjs-torrent.min.js",
    "seed": "webtorrent-hybrid seed test/fixtures/bodyColor.css test/fixtures/multiple test/fixtures/pathTest test/fixtures/syncTest test/fixtures/windowString.js"
  },
  "main": "loadjs-torrent.js",
  "keywords": [
    "loadjs",
    "torrent",
    "webtorrent",
    "peer",
    "p2p",
    "load",
    "balance"
  ],
  "repository": "https://github.com/kayleepop/loadjs-torrent.git",
  "bugs": {
    "type": "git",
    "url": "https://github.com/kayleepop/loadjs-torrent/issues"
  },
  "devDependencies": {
    "babel-minify": "^0.3.0",
    "browserify": "^16.1.1",
    "standard": "^11.0.1",
    "tape": "^4.6.3",
    "tape-run": "^3.0.1",
    "webtorrent-hybrid": "^1.0.6"
  },
  "dependencies": {
    "glob-to-regexp": "^0.4.0",
    "indexeddb-chunk-store": "^2.2.1",
    "loadjs": "^3.5.3",
    "webtorrent": "^0.98.18"
  },
  "standard": {
    "ignore": [
      "test/fixtures/"
    ]
  }
}
