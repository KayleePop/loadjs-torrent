{
  "name": "loadjs-torrent",
  "version": "3.0.0",
  "author": "Kayleepop",
  "license": "MIT",
  "description": "An extension of loadjs that supports torrents",
  "scripts": {
    "test": "standard && browserify test/*.js | tape-run --static test/fixtures",
    "build": "browserify loadjs-torrent.js -s torrentJs | minify > loadjs-torrent.min.js"
  },
  "main": "loadjs-torrent.js",
  "keywords": [
    "loadjs",
    "torrent",
    "webtorrent",
    "peer",
    "p2p",
    "load",
    "balance"
  ],
  "repository": "https://github.com/kayleepop/loadjs-torrent.git",
  "bugs": {
    "type": "git",
    "url": "https://github.com/kayleepop/loadjs-torrent/issues"
  },
  "devDependencies": {
    "babel-minify": "^0.4.0",
    "browserify": "^16.1.1",
    "tape-promise": "^3.0.0",
    "tape": "^4.9.1",
    "standard": "^11.0.1",
    "tape-run": "^4.0.0"
  },
  "dependencies": {
    "glob-to-regexp": "^0.4.0",
    "loadjs": "^3.5.3",
    "webtorrent": "https://github.com/KayleePop/webtorrent/tarball/idbkv-chunk-store"
  },
  "standard": {
    "ignore": [
      "test/fixtures/"
    ]
  }
}
